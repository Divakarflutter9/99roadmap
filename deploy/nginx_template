server {
    listen 80;
    server_name your_domain_or_ip;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /var/www/99roadmap;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/var/www/99roadmap/99roadmap.sock;
    }
}
